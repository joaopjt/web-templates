    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "glob": "^7.1.2",
    "hapi": "^18.1.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "inert": "^5.1.3",
    "laravel-mix": "^5.0.4",
    "laravel-mix-nunjucks-2": "^1.0.0",
